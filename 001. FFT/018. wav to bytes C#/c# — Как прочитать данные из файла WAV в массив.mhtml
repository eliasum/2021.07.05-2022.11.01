<pre><code>// convert two bytes to one double in the range -1 to 1
static double bytesToDouble(byte firstByte, byte secondByte) {
    // convert two bytes to one short (little endian)
    short s =3D (secondByte &lt;&lt; 8) | firstByte;
    // convert to range from -1 to (just below) 1
    return s / 32768.0;
}

// Returns left and right double arrays. 'right' will be null if sound is m=
ono.
public void openWav(string filename, out double[] left, out double[] right)
{
    byte[] wav =3D File.ReadAllBytes(filename);

    // Determine if mono or stereo
    int channels =3D wav[22];     // Forget byte 23 as 99.999% of WAVs are =
1 or 2 channels

    // Get past all the other sub chunks to get to the data subchunk:
    int pos =3D 12;   // First Subchunk ID from 12 to 16

    // Keep iterating until we find the data chunk (i.e. 64 61 74 61 ......=
 (i.e. 100 97 116 97 in decimal))
    while(!(wav[pos]=3D=3D100 &amp;&amp; wav[pos+1]=3D=3D97 &amp;&amp; wav[=
pos+2]=3D=3D116 &amp;&amp; wav[pos+3]=3D=3D97)) {
        pos +=3D 4;
        int chunkSize =3D wav[pos] + wav[pos + 1] * 256 + wav[pos + 2] * 65=
536 + wav[pos + 3] * 16777216;
        pos +=3D 4 + chunkSize;
    }
    pos +=3D 8;

    // Pos is now positioned to start of actual sound data.
    int samples =3D (wav.Length - pos)/2;     // 2 bytes per sample (16 bit=
 sound mono)
    if (channels =3D=3D 2) samples /=3D 2;        // 4 bytes per sample (16=
 bit stereo)

    // Allocate memory (right will be null if only mono sound)
    left =3D new double[samples];
    if (channels =3D=3D 2) right =3D new double[samples];
    else right =3D null;

    // Write to double array/s:
    int i=3D0;
    while (pos &lt; length) {
        left[i] =3D bytesToDouble(wav[pos], wav[pos + 1]);
        pos +=3D 2;
        if (channels =3D=3D 2) {
            right[i] =3D bytesToDouble(wav[pos], wav[pos + 1]);
            pos +=3D 2;
        }
        i++;
    }
}
