<Macro>
  <Item key="Element">
    <Channel title="Канал" description="Канал электронной платы" />
    <Version title="Модификация" description="Версия модификации электронной платы" />
    <Adress title="Адрес" description="Адрес электронной платы" />
    <Amperage title="Ток" description="Значение тока" symbol="I =" unit="[А]" Allowance="1" />
    <Chart title="График" description="График заданного параметра" />
    <Widget title="Настройка" description="Настройка виджета пользователя" />
    <Scale title="Масштаб" description="Масштаб отображения" />
    <Temperature title="Температура" description="Значение температуры" symbol="T =" unit="[°C]" Allowance="1" />
    <Voltage title="Напряжение" description="Значение напряжения" symbol="U =" unit="[В]" Allowance="1" />
  </Item>
  <Item key="Attribute">
    <Locking title="Блокировка" const="?" description="Блокировка канала электронной платы" />
    <Reaction title="Время реакции" const="?" description="Времени реакции канала для уставки заданного значения тока" />
    <Holding title="Ток удержания" const="?" description="Значение тока удержания" />
    <Allowance title="Допуск" const="?" description="Значение допуска" symbol="Δ" unit="[%]" />
    <Limit title="Предел" description="Предельное значение">
      <Template key="?" title="{../@title} № {@key}" description="{../@description} № {@key}">
        <Minimum title="Минимум" const="?" description="Минимально предельное значение" />
        <Maximum title="Максимум" const="?" description="Максимально предельное значение" />
      </Template>
    </Limit>
  </Item>
</Macro>